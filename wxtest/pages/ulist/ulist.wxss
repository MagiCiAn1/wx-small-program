/* pages/ulist/ulist.wxss */
.navigator-item{
  display:flex;
  flex-direction:row;
}
.viPhoto{
  flex: 2;
  height: 150rpx; overflow: hidden;
}
.poster{
  width: 100%; height: 150rpx; z-index: 8;
}
.noPhoto{
  width: 100%; background: #EEEEEE; height: 150rpx; line-height: 150rpx; text-align: center; color: #999; font-size: 26rpx
}
.wiPhoto{
  width: 100%; height: 150rpx; overflow: hidden; position: relative; 
}
.textCms{
  flex: 4; width: 97%; color: #333; padding-left: 3%; overflow: hidden
}

.touch-item {
 display: flex;
 justify-content: space-between;
 width: 100%;
 overflow: hidden;
 border-bottom: 2rpx #f5f5f5 solid;
}
.content {
 width: 100%;
 padding: 15px;
 margin-right:0;
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 margin-left: -90px
}

.del {
 background-color: orangered;
 width: 90px;
 display: flex;
 flex-direction: column;
 align-items: center;
 justify-content: center;
 color: #fff;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
}

.touch-move-active .content,
.touch-move-active .del {
 -webkit-transform: translateX(0);
 transform: translateX(0);
}
.text{
  font-size: 35rpx;
  color: #C0C0C0;
  width: 100%;
  display: inline-block;
  text-align: center;
  padding-top:45rpx 
}


